{
    "name": "start",
    "description": "My new tab page",
    "permissions": [
        "bookmarks"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "version": "3.1",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "manifest_version": 3,
    "background": {
        "service_worker": "bg.js",
        "type": "module"
    }
}
